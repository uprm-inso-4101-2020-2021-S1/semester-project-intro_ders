@page
@model intro_durs.Pages.Application.PlanillaContribucionReportadosModel
@{
}

@using intro_durs.Pages.Models

<!DOCTYPE html>

<head>
</head>


<html>
<body>

    <center>
        <h2>Solicitudes de Planillas de Contribucion</h2>
        <p>&nbsp;</p>




        <table class="table table-bordered table-hover" style="margin-left:-85px">
            <thead class="thead-dark">
                <tr class="info">
                    <th style="text-align:center">Full Name</th>
                    <th style="text-align:center">Email</th>
                    <th style="text-align:center">Phone</th>
                    <th style="text-align:center">Active or Inactive</th>                    <th style="text-align:center">Student Program</th>
                    <th style="text-align:center">Student Program</th>
                    <th style="text-align:center">Signature</th>
                    <th style="text-align:center">Date</th>


                </tr>
            </thead>
            @foreach (var i in ViewData["Tbl"] as IList<spPlanillaContribucion>)
            {
                <tr class="active">

                    <td>@i.strFullName</td>
                    <td>@i.strEmail</td>
                    <td>@i.strPhone</td>
                    <td>@i.strActiveOrInactive</td>
                    <td>@i.strStudentProgram</td>
                    <td>@i.strSignature</td>
                    <td>@i.strDate</td>

                </tr>


            }





        </table>
    </center>

</body>



</html>

